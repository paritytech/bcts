{
  "name": "@blockchain-commons/known-values",
  "version": "1.0.0-alpha.2",
  "type": "module",
  "description": "Blockchain Commons Known Values for TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "browser": "dist/index.iife.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.iife.js",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
  },
  "keywords": [
    "known-values",
    "blockchain-commons",
    "ontology",
    "encoding",
    "cbor",
    "dcbor",
    "deterministic"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@blockchain-commons/dcbor": "workspace:*",
    "@blockchain-commons/tags": "workspace:*"
  },
  "devDependencies": {
    "@blockchain-commons/eslint": "workspace:*",
    "@blockchain-commons/tsconfig": "workspace:*",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.2",
    "@types/pako": "^2.0.4",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsdown": "^0.17.2",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  }
}
